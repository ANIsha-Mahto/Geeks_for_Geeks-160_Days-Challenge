class Solution {
  public:
    int kthMissing(vector<int> &arr, int k) {
        unordered_set<int>s(arr.begin(),arr.end());
        int count=0,curr=0;
        while(count<k){
            curr+=1;
            if(s.find(curr)==s.end()){
                count++;
            }
        }
        return curr;
        
    }
};
